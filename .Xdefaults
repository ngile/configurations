URxvt.intensityStyles:	false
URxvt*inheritPixmap: true
URxvt*transparent: true
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*shading: 15
!URxvt.background:       [90]#000000
URxvt.depth:		32
URxvt.font: 		xft:Terminus:size=13, xft:Inconsolata\ for\ Powerline:size=13
URxvt.boldfont:     xft:Terminus:pixelsize=13
Xft.antialias:  true

URxvt.scrollBar:	false
URxvt.cursorColor:	#89d1ec
URxvt.cursorUnderline: 1
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,keyboard-select,resize-font
URxvt.matcher.button:	1
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher:   /usr/bin/qutebrowser
URxvt.underlineURLs:  True
URxvt.matcher.button: 1 
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V:     perl:pasta:paste
! Comment this if you don't want copy when text is selected
URxvt.clipboard.autocopy: true

URxvt.colorUL:		#4682B4



!Xterm escapes, word by word movement
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C




